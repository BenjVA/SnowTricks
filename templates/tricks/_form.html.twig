{{ form_start(form) }}
    {{ form_row(form.name) }}
    {{ form_row(form.description) }}
    {{ form_row(form.groupTricks) }}
    {{ form_row(form.images) }}

<div {{ stimulus_controller('form-collection') }}
        data-form-collection-index-value="{{ form.videos|length > 0 ? form.videos|last.vars.name + 1 : 0 }}"
        data-form-collection-prototype-value="{{ form_widget(form.videos.vars.prototype)|e('html_attr') }}"
>
    <ul {{ stimulus_target('form-collection', 'collectionContainer') }}></ul>
    <button type="button" {{ stimulus_action('form-collection', 'addCollectionElement') }}>Ajouter un lien</button>
</div>
    <button type="submit" class="btn btn-outline-dark mb-3">Valider</button>
{{ form_end(form) }}
